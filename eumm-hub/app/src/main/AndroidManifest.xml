<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ufipay.eummhub">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_eumm_client_3_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_eumm_client_3_foreground"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".core.activities.ProfilUtilisateur"
                  android:screenOrientation="portrait"
                  android:theme="@style/AppTheme2"/>
        <activity
            android:name=".core.activities.Launcher"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".core.activities.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".core.activities.Pager"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".core.activities.Authentification"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme2" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>